        <Button x:Name="btnSearchTrainee_Copy" Content="חיפוש בוחן" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Click="btnSearchTester_Click" Height="93" Margin="274,179,0,0" />
    </Grid>
</Window>
